local a=game:GetService("RunService")local b=game:GetService("Players").LocalPlayer;local c=b.Character;c.Archivable=true;local d=workspace:WaitForChild("Camera")local e=c:Clone()e.Head.face.Texture=""e.Parent=workspace;d.CameraSubject=e:FindFirstChildOfClass("Humanoid")local f={}local g=false;local function h(i,j,k,l)i.CanCollide=false;local m=Instance.new("AlignPosition")m.ApplyAtCenterOfMass=true;m.MaxForce=100000;m.MaxVelocity=math.huge;m.ReactionForceEnabled=false;m.Responsiveness=200;m.RigidityEnabled=false;m.Parent=i;local n=Instance.new("AlignOrientation")n.MaxAngularVelocity=math.huge;n.MaxTorque=100000;n.PrimaryAxisOnly=false;n.ReactionTorqueEnabled=false;n.Responsiveness=200;n.RigidityEnabled=false;n.Parent=i;local o=Instance.new("Attachment")o.Parent=i;local p=Instance.new("Attachment")p.Parent=j;p.Orientation=l;p.Position=k;m.Attachment0=o;m.Attachment1=p;n.Attachment0=o;n.Attachment1=p end;local q,r=pcall(function()for s,t in pairs(e:GetDescendants())do if t:IsA("Part")then t.Transparency=1 elseif t:IsA("SpecialMesh")then t.MeshId="rbxassetid://0"end end;spawn(function()while a.Stepped:wait()and not g do settings().Physics.AllowSleep=false;sethiddenproperty(b,"MaximumSimulationRadius",math.pow(math.huge,math.huge)*math.huge)sethiddenproperty(b,"SimulationRadius",math.pow(math.huge,math.huge)*math.huge)end end)table.insert(f,a.Stepped:Connect(function()e:FindFirstChild("Head").CanCollide=false;e:FindFirstChild("Torso").CanCollide=false;c.Head.CanCollide=false;c.Torso.CanCollide=false end))table.insert(f,a.Heartbeat:Connect(function()e:FindFirstChild("Head").CanCollide=false;e:FindFirstChild("Torso").CanCollide=false;c.Head.CanCollide=false;c.Torso.CanCollide=false end))table.insert(f,a.RenderStepped:Connect(function()e:FindFirstChild("Head").CanCollide=false;e:FindFirstChild("Torso").CanCollide=false;c.Head.CanCollide=false;c.Torso.CanCollide=false end))c.Torso["Right Shoulder"]:Destroy()c.Torso["Left Shoulder"]:Destroy()c.Torso["Right Hip"]:Destroy()c.Torso["Left Hip"]:Destroy()c.HumanoidRootPart["RootJoint"]:Destroy()c.HumanoidRootPart.Anchored=true;c.Humanoid.PlatformStand=true;e["Torso"].Position=c["Torso"].Position;for s,t in pairs(c:GetChildren())do if t:IsA("Part")and t.Name~="Head"then if t.Name=="Torso"then h(c[t.Name],e[t.Name],Vector3.new(0,0.5,0),Vector3.new(0,0,0))else h(c[t.Name],e[t.Name],Vector3.new(0,0,0),Vector3.new(0,0,0))end end end end)if r then warn(r)b.Character=c;c:BreakJoints()e:Destroy()g=true;return end;e.Humanoid.Died:Connect(function()b.Character=c;c:BreakJoints()e:Destroy()g=true;for s,t in pairs(f)do t:Disconnect()end end)c.Humanoid.Died:Connect(function()b.Character=c;c:BreakJoints()e:Destroy()g=true;for s,t in pairs(f)do t:Disconnect()end end)b.Character=e
